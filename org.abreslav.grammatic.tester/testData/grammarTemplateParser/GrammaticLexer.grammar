LETTER : ['a'--'z' 'A'--'Z' '_'];
WS : [0x0000--0x0020]+;
COMMENT : '//' ([^'\n'])* '\n';
NAME : LETTER (LETTER | ['0'--'9'])*;
CHAR_IN_QUOTES : ('\\'[^'\n' '\r']) | [^'\\' '\n'  '\r'  '\''];
CHARACTER : '\'' CHAR_IN_QUOTES '\'';
STRING : '\'' CHAR_IN_QUOTES CHAR_IN_QUOTES+ '\'';
HEX_DIGIT : ['0'--'9' 'a'--'f' 'A'--'F'];
CODE : '0x' HEX_DIGIT HEX_DIGIT HEX_DIGIT HEX_DIGIT;
INT : '-'? ['0'--'9']+;
