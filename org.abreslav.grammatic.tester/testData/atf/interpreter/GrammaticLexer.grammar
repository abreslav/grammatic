WS {lexical; whitespace} : [0x0000--0x0020]+;
COMMENT {lexical; whitespace} : '//' ([^'\n'])* '\n';
LETTER {lexical; fragment} : ['a'--'z' 'A'--'Z' '_'];
NAME {lexical} : LETTER (LETTER | ['0'--'9'])*;
CHAR_IN_QUOTES {lexical; fragment} : ('\\'[^'\n' '\r']) | [^'\\' '\n'  '\r'  '\''];
CHARACTER {lexical} : '\'' CHAR_IN_QUOTES '\'';
STRING {lexical} : '\'' CHAR_IN_QUOTES CHAR_IN_QUOTES+ '\'';
HEX_DIGIT {lexical; fragment} : ['0'--'9' 'a'--'f' 'A'--'F'];
CODE {lexical} : '0x' HEX_DIGIT HEX_DIGIT HEX_DIGIT HEX_DIGIT;
INT {lexical} : '-'? ['0'--'9']+;
