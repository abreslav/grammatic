#javaoptions {
	grammarName = 'GrammaticLexer';
	grammarPackage = 'org.abreslav.grammatic.grammar1';
}
  
WS : #token(whitespace); 
COMMENT : #token(whitespace);
LETTER : #token(fragment);
NAME : #token;
CHAR_IN_QUOTES : #token(fragment);
CHARACTER : #token;
STRING : #token;
hexDigit : #token(fragment);
CODE : #token;
INT : #token;