<?xml version="1.0" encoding="ASCII"?>
<instantiator-test:InstantiatorTests xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:grammatic-grammar="http://www.abreslav.org/Grammatic/2008/Grammar" xmlns:grammatic-grammar-template="http://www.abreslav.org/Grammatic/2008/GrammarTemplate" xmlns:instantiator-test="http://www.abreslav.org/grammartic/2008/tests/instantiator" xmlns:template="http://www.abreslav.org/Grammatic/2008/Template" xsi:schemaLocation="http://www.abreslav.org/Grammatic/2008/Grammar ../../../org.abreslav.grammatic.grammar/model/grammar.ecore http://www.abreslav.org/grammartic/2008/tests/instantiator ../../model/instantiator_test.ecore">
  <tests xsi:type="instantiator-test:InstantiatorTest" text="simple">
    <template name="x">
      <body xsi:type="grammatic-grammar-template:GrammarTemplate"/>
    </template>
    <templateApplication template="//@tests.0/@template"/>
    <data xsi:type="grammatic-grammar:Grammar"/>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="one subobject">
    <template name="x">
      <body xsi:type="grammatic-grammar-template:GrammarTemplate">
        <symbols xsi:type="grammatic-grammar-template:SymbolTemplate"/>
      </body>
    </template>
    <templateApplication template="//@tests.1/@template"/>
    <data xsi:type="grammatic-grammar:Grammar">
      <symbols/>
    </data>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="many subobjects">
    <template name="x">
      <body xsi:type="grammatic-grammar-template:GrammarTemplate">
        <symbols xsi:type="grammatic-grammar-template:SymbolTemplate">
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate">
            <expression xsi:type="grammatic-grammar-template:SequenceTemplate">
              <expressions xsi:type="grammatic-grammar-template:EmptyTemplate"/>
              <expressions xsi:type="grammatic-grammar-template:EmptyTemplate"/>
            </expression>
          </productions>
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate"/>
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate">
            <expression xsi:type="grammatic-grammar-template:EmptyTemplate"/>
          </productions>
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate"/>
        </symbols>
      </body>
    </template>
    <templateApplication template="//@tests.2/@template"/>
    <data xsi:type="grammatic-grammar:Grammar">
      <symbols>
        <productions>
          <expression xsi:type="grammatic-grammar:Sequence">
            <expressions xsi:type="grammatic-grammar:Empty"/>
            <expressions xsi:type="grammatic-grammar:Empty"/>
          </expression>
        </productions>
        <productions/>
        <productions>
          <expression xsi:type="grammatic-grammar:Empty"/>
        </productions>
        <productions/>
      </symbols>
    </data>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="attributes">
    <template name="x">
      <body xsi:type="grammatic-grammar-template:GrammarTemplate">
        <symbols xsi:type="grammatic-grammar-template:SymbolTemplate" name="name">
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate">
            <expression xsi:type="grammatic-grammar-template:IterationTemplate" lowerBound="5" upperBound="6"/>
          </productions>
        </symbols>
      </body>
    </template>
    <templateApplication template="//@tests.3/@template"/>
    <data xsi:type="grammatic-grammar:Grammar">
      <symbols name="name">
        <productions>
          <expression xsi:type="grammatic-grammar:Iteration" lowerBound="5" upperBound="6"/>
        </productions>
      </symbols>
    </data>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="applications_to_be_used">
    <template name="iteration">
      <body xsi:type="grammatic-grammar-template:IterationTemplate" lowerBound="1" upperBound="2"/>
    </template>
    <templateApplication template="//@tests.4/@template"/>
    <data xsi:type="grammatic-grammar:Iteration" lowerBound="1" upperBound="2"/>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="applications">
    <template name="iteration_i">
      <body xsi:type="grammatic-grammar-template:IterationTemplate" lowerBound="1" upperBound="2">
        <expression xsi:type="template:TemplateApplication" template="//@tests.4/@template"/>
      </body>
    </template>
    <templateApplication template="//@tests.5/@template"/>
    <data xsi:type="grammatic-grammar:Iteration" lowerBound="1" upperBound="2">
      <expression xsi:type="grammatic-grammar:Iteration" lowerBound="1" upperBound="2"/>
    </data>
  </tests>
  <tests xsi:type="instantiator-test:InstantiatorTest" text="parameters">
    <template name="p">
      <parameters name="single">
        <type href="../../../org.abreslav.grammatic.grammar/model/grammar.ecore#//Symbol"/>
      </parameters>
      <parameters name="many" upperBound="-1">
        <type href="../../../org.abreslav.grammatic.grammar/model/grammar.ecore#//Production"/>
      </parameters>
      <body xsi:type="grammatic-grammar-template:GrammarTemplate">
        <symbols xsi:type="template:ParameterReference" parameter="//@tests.6/@template/@parameters.0"/>
        <symbols xsi:type="grammatic-grammar-template:SymbolTemplate">
          <productions xsi:type="grammatic-grammar-template:ProductionTemplate">
            <expression xsi:type="grammatic-grammar-template:EmptyTemplate"/>
          </productions>
          <productions xsi:type="template:ParameterReference" parameter="//@tests.6/@template/@parameters.1"/>
        </symbols>
      </body>
    </template>
    <templateApplication template="//@tests.6/@template">
      <arguments parameter="//@tests.6/@template/@parameters.0">
        <values xsi:type="template:ObjectContainer">
          <object xsi:type="grammatic-grammar:Symbol"/>
        </values>
      </arguments>
      <arguments parameter="//@tests.6/@template/@parameters.1">
        <values xsi:type="template:ObjectContainer">
          <object xsi:type="grammatic-grammar:Production">
            <expression xsi:type="grammatic-grammar:CharacterRange"/>
          </object>
        </values>
        <values xsi:type="template:ObjectContainer">
          <object xsi:type="grammatic-grammar:Production">
            <expression xsi:type="grammatic-grammar:Sequence"/>
          </object>
        </values>
      </arguments>
    </templateApplication>
    <data xsi:type="grammatic-grammar:Grammar">
      <symbols xsi:type="grammatic-grammar:Symbol"/>
      <symbols>
        <productions>
          <expression xsi:type="grammatic-grammar:Empty"/>
        </productions>
        <productions xsi:type="grammatic-grammar:Production">
          <expression xsi:type="grammatic-grammar:CharacterRange"/>
        </productions>
        <productions xsi:type="grammatic-grammar:Production">
          <expression xsi:type="grammatic-grammar:Sequence"/>
        </productions>
      </symbols>
    </data>
  </tests>
</instantiator-test:InstantiatorTests>
