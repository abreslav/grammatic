stateMachine
  : "statemachine" Name "{" state* "}"
  ;

state
  : "start"? "end"? "state" Name "{" action* "}";

action
  : "case" Int ":" Name ";"
  ;

Int : Digit+;
Digit : ['0'-'9'];

Name : NameStartCharacter NameCharacter*;
NameCharacter : NameStartCharacter | Digit;
NameStartCharacter : ['A'-'Z'] | ['a'-'z'] | '_';